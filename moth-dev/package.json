{
  "name": "moth-dev",
  "displayName": "Moth Dev",
  "description": "VS Code support for the Moth language.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "moth",
      "aliases": ["Moth", "moth"],
      "extensions": ["moth"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "moth",
      "scopeName": "source.moth",
      "path": "./syntaxes/moth.tmLanguage.json"
    }]
  }
}
